<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ButtonStyle">
        <item name="android:textSize">@dimen/_16ssp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/text_color</item>
        <item name="cornerRadius">@dimen/_8sdp</item>
        <item name="backgroundTint">@color/primary_color</item>
        <item name="elevation">@dimen/_8sdp</item>
    </style>

    <style name="ItemButtonStyle">
        <item name="android:textSize">@dimen/_12ssp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/text_color</item>
        <item name="cornerRadius">@dimen/_16sdp</item>
        <item name="backgroundTint">@color/primary_color</item>
        <item name="elevation">@dimen/_8sdp</item>
    </style>

    <style name="CornerCut" parent="">
        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerFamilyBottomRight">rounded</item>
        <item name="cornerSizeBottomLeft">@dimen/_16sdp</item>
        <item name="cornerSizeBottomRight">@dimen/_16sdp</item>
    </style>

    <style name="Circle" parent="">
        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerFamilyBottomRight">rounded</item>
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">rounded</item>
        <item name="cornerSizeBottomLeft">@dimen/_16sdp</item>
        <item name="cornerSizeBottomRight">@dimen/_16sdp</item>
        <item name="cornerSizeTopLeft">@dimen/_16sdp</item>
        <item name="cornerSizeTopRight">@dimen/_16sdp</item>
    </style>

    <style name="MyTextView" parent="">
        <item name="android:textSize">@dimen/_16ssp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="MyTextView12" parent="">
        <item name="android:textSize">@dimen/_14ssp</item>
        <item name="android:textColor">@color/black</item>
    </style>
    
</resources>